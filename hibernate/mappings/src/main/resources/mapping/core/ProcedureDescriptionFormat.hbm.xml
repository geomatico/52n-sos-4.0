<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.ProcedureDescriptionFormat" table="procedure_description_format">
        <id name="procedureDescriptionFormatId" type="long">
            <column name="procedure_description_format_id" />
            <generator class="sequence">
            	<param name="sequence">procedure_description_format_id_seq</param>
            </generator>
        </id>
        <property name="procedureDescriptionFormat" type="string">
            <column name="procedure_description_format" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
