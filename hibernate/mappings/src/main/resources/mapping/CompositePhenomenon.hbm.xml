<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.CompositePhenomenon" table="composite_phenomenon">
        <id name="compositePhenomenonId" type="long">
            <column name="composite_phenomenon_id" />
            <generator class="sequence">
            	<param name="sequence">composite_phenomenon_id_seq</param>
            </generator>
        </id>
        <property name="identifier" type="string">
            <column name="identifier" not-null="true" unique="true" />
        </property>
        <property name="description" type="string">
            <column name="description" />
        </property>
        <set name="requests" table="request_has_composite_phenomenon" inverse="true" lazy="true" fetch="join">
            <key>
                <column name="composite_phenomenon_id" not-null="true" />
            </key>
            <many-to-many entity-name="org.n52.sos.ds.hibernate.entities.Request">
                <column name="request_id" not-null="true" />
            </many-to-many>
        </set>
        <set name="observableProperties" table="composite_phenomenon_has_observable_property" inverse="false" lazy="true" fetch="join">
            <key>
                <column name="composite_phenomenon_id" not-null="true" />
            </key>
            <many-to-many entity-name="org.n52.sos.ds.hibernate.entities.ObservableProperty">
                <column name="observable_property_id" not-null="true" />
            </many-to-many>
        </set>
    </class>
</hibernate-mapping>
