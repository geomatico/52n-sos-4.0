<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.Quality" table="quality">
        <id name="qualityId" type="long">
            <column name="quality_id" />
            <generator class="sequence">
            	<param name="sequence">quality_id_seq</param>
            </generator>
        </id>
        <many-to-one name="sweType" class="org.n52.sos.ds.hibernate.entities.SweType" fetch="join">
            <column name="swe_type_id" not-null="true" />
        </many-to-one>
        <many-to-one name="unit" class="org.n52.sos.ds.hibernate.entities.Unit" fetch="join">
            <column name="unit_id" not-null="true" />
        </many-to-one>
        <property name="name" type="string">
            <column name="name" not-null="true" />
        </property>
        <property name="value" type="string">
            <column name="value" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
