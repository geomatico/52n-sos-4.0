<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.SpatialFilteringProfile" table="spatial_filtering_profile">
        <id name="spatialFilteringProfileId" type="long">
            <column name="spatial_filtering_profile_id" />
            <generator class="sequence">
            	<param name="sequence">spatial_filtering_profile_id_seq</param>
            </generator>
        </id>
        <property name="identifier" type="string">
            <column name="identifier" not-null="true" unique="true" />
        </property>
        <property name="geom" type="org.hibernate.spatial.GeometryType">
            <column name="geom" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
