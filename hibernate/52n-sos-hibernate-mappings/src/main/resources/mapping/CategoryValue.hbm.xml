<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.CategoryValue" table="category_value">
        <id name="categoryValueId" type="long">
            <column name="category_value_id" />
            <generator class="sequence">
            	<param name="sequence">category_value_id_seq</param>
            </generator>
        </id>
        <property name="value" type="string">
            <column name="value" not-null="true" unique="true" />
        </property>
    </class>
</hibernate-mapping>
