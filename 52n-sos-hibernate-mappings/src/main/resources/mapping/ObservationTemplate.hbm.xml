<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.ObservationTemplate" table="observation_template">
        <id name="observationTemplateId" type="long">
            <column name="observation_template_id" />
            <generator class="sequence">
            	<param name="sequence">observation_template_id_seq</param>
            </generator>
        </id>
        <property name="observationTemplate" type="string">
            <column name="observation_template" />
        </property>
    </class>
</hibernate-mapping>
