<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.sos.ds.hibernate.entities.SweType" table="swe_type">
        <id name="sweTypeId" type="long">
            <column name="swe_type_id" />
            <generator class="sequence">
            	<param name="sequence">swe_type_id_seq</param>
            </generator>
        </id>
        <property name="sweType" type="string">
            <column name="swe_type" not-null="true" unique="true" />
        </property>
    </class>
</hibernate-mapping>
